{
  "coc.preferences.jumpCommand": "tab drop",
  "html.autoClosingTags": true,
  "diagnostic.warningSign": "!",
  "explorer.file.showHiddenFiles": true,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "<cr>": [
     "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ],
    ">": ["expandable?", "cd", "open"],
    "<": "gotoParent"
  },
  "git.stagedDecorator": "^",
  "git.realtimeGutters": false,
  "git.addedSign.text": "▍",
  "git.addedSign.hlGroup": "DiffAdd",
  "git.removedSign.text": "▁",
  "git.removedSign.hlGroup": "DiffDelete",
  "git.topRemovedSign.text": "▔",
  "git.topRemovedSign.hlGroup": "DiffTopDelete",
  "git.changedSign.text": "▍",
  "git.changedSign.hlGroup": "DiffChange",
  "git.changeRemovedSign.text": "▞",
  "git.changeRemovedSign.hlGroup": "DiffChangeDelete"
}
